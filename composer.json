{
    "name": "migratetoflarum/renumber-posts",
    "description": "Re-number posts on the forum",
    "keywords": [
        "extension",
        "flarum",
        "migration",
        "number"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/"
        }
    ],
    "support": {
        "source": "https://github.com/migratetoflarum/renumber-posts"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.16"
    },
    "extra": {
        "flarum-extension": {
            "title": "Renumber Posts",
            "category": "feature",
            "icon": {
                "name": "fas fa-sort-numeric-down",
                "backgroundColor": "#e7672e",
                "color": "#fafafa"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "MigrateToFlarum\\RenumberPosts\\": "src/"
        }
    }
}
